{{#> _Layout title="Línea de tiempo" active="timeline"}}

<h1 class="text-2xl font-bold mb-6">Mi Línea de Tiempo</h1>

<ul class="timeline space-y-4">
    {{#each Items}}
    <li>
        <p class="font-semibold">{{Fecha}} — {{Titulo}}</p>
        <p class="text-gray-600">{{Descripcion}}</p>
    </li>
    {{/each}}
</ul>

{{/_Layout}}

